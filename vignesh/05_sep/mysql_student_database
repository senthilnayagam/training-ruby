Created Table:


create table student(
    -> RollNo integer primary key,
    -> name varchar(32),
    -> DOB date,
    -> subject1 integer,
    -> subject2 integer,
    -> subject3 integer,
    -> subject4 integer,
    -> subject5 integer,
    -> subject6 integer,
    -> Total integer,
    -> Avg float(3,2));


